import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int h = sc.nextInt();
        double v = sc.nextDouble();
        double salario = h*v;
        System.out.println("NUMBER = "+n);
        System.out.printf("SALARY = U$ %.2f\n",salario);

}   
}
