import java.util.Scanner;

class Main {
          public static void main(String[] args) {
        
          Scanner entrada = new Scanner(System.in);
        int n = entrada.nextInt();
        int anos = n/365;
        int meses = n%365;
        int dias = meses%30;
        meses = meses/30;
        System.out.println(anos+" ano(s)\n"+meses+" mes(es)\n"+dias+" dia(s)");

    }
}
